<% if room.owner != current_user.id %>
  <% if get_relationship_id(current_user).status == 0 %>
    <button class="btn btn-success follow" data-relation-id="<%= get_relationship_id(current_user).id %>">Sẵn sàng</button>
  <% else %>
    <button class="btn btn-success unfollow"data-relation-id="<%= get_relationship_id(current_user).id %>">Không sẵn sàng</button>
  <% end %>
<% else %>
  <%= link_to "Bắt đầu",exames_path, class: "btn btn-success" %>
<% end %>
